{
  "editor.fontFamily": "'Jetbrains Mono'",
  "editor.fontSize": 16,
  "editor.tabSize": 2,
  "editor.minimap.enabled": false,
  "editor.fontLigatures": true,
  "editor.renderWhitespace": "boundary",
  "editor.lineNumbers": "relative",
  "editor.autoClosingBrackets": "languageDefined",
  "git.decorations.enabled": true,
  "workbench.iconTheme": "vscode-icons",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "window.menuBarVisibility": "toggle",
  "keyboard.dispatch": "keyCode",
  "csharp.referencesCodeLens.enabled": false,
  "vim.leader": "<space>",
  "vim.normalModeKeyBindings": [
    {
      "before": [
        "0",
      ],
      "after": [
        "^"
      ]
    },
    {
      "before": [
        ";"
      ],
      "commands": [
        "vim.showQuickpickCmdLine",
      ]
    },
    {
      "before": [
        "g",
        "d"
      ],
      "commands": [
        "editor.action.goToImplementation"
      ]
    },
    {
      "before": [
        "g",
        "y"
      ],
      "commands": [
        "editor.action.goToTypeDefinition"
      ]
    },
  ],
  // "sync.gist": "0f51f60880f3c9b4ed3c0fab7db02810",
  "python.pythonPath": "${workspaceFolder}/.venv",
  "java.configuration.checkProjectSettingsExclusions": false,
  "material-icon-theme.folders.associations": {
    "actions": "Redux-actions",
    "store": "Redux-store",
    "components": "React-components",
    "requests": "Other"
  },
  "editor.formatOnSave": true,
  "[typescript]": {
    "editor.formatOnSave": true
  },
  "[typescriptreact]": {
    "editor.formatOnSave": true
  },
  "[csharp]": {
    "editor.formatOnSave": true
  },
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
  "javascript.preferences.quoteStyle": "double",
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
  "javascript.format.placeOpenBraceOnNewLineForFunctions": true,
  "workbench.startupEditor": "newUntitledFile",
  "explorer.confirmDelete": false,
  "terminal.integrated.shell.linux": "/usr/bin/fish",
  "terminal.explorerKind": "external",
  "terminal.integrated.cursorBlinking": true,
  "terminal.external.linuxExec": "gnome-terminal",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "python.jediEnabled": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "omnisharp.path": "latest",
  "vscode-neovim.neovimPath": "/usr/bin/nvim",
  "workbench.colorTheme": "Gruvbox Dark Hard",
  "terminal.integrated.rendererType": "dom",
  "omnisharp.useGlobalMono": "always",
  "vsicons.dontShowNewVersionMessage": true,
  "workbench.colorCustomizations": {
    "editor.background": "#0d0b0e",
    "activityBar.activeBackground": "#0d0b0e",
    "activityBar.background": "#0d0b0e",
    "tab.inactiveBackground": "#0d0b0e",
    "panel.background": "#0d0b0e",
    "statusBar.background": "#0d0b0e",
    "[Wal]": {
      "focusBorder": "#B0B0B1",
      "foreground": "#e8e6e6",
      "widget.shadow": "#a2a1a1",
      "selection.background": "#807E78",
      "descriptionForeground": "#B0B0B1",
      "errorForeground": "#807E78",
      "input.background": "#0F140B",
      "input.foreground": "#e8e6e6",
      "input.placeholderForeground": "#a2a1a1",
      "inputOption.activeBorder": "#B0B0B1",
      "inputValidation.errorBackground": "#807E78",
      "inputValidation.errorBorder": "#807E78",
      "inputValidation.infoBackground": "#B0B0B1",
      "inputValidation.infoBorder": "#B0B0B1",
      "inputValidation.warningBackground": "#969592",
      "inputValidation.warningBorder": "#969592",
      "list.invalidItemForeground": "#807E78",
      "list.errorForeground": "#807E78",
      "list.warningForeground": "#969592",
      "terminal.background": "#0F140B",
      "terminal.foreground": "#e8e6e6",
      "terminalCursor.background": "#0F140B",
      "terminalCursor.foreground": "#e8e6e6",
      "terminal.ansiBlack": "#0F140B",
      "terminal.ansiRed": "#807E78",
