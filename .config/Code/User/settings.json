      "terminal.ansiCyan": "#D3D2D3",
      "terminal.ansiWhite": "#e8e6e6",
      "terminal.ansiBrightBlack": "#a2a1a1",
      "terminal.ansiBrightRed": "#807E78",
      "terminal.ansiBrightGreen": "#868579",
      "terminal.ansiBrightYellow": "#969592",
      "terminal.ansiBrightBlue": "#B0B0B1",
      "terminal.ansiBrightMagenta": "#BFBEC1",
      "terminal.ansiBrightCyan": "#D3D2D3",
      "terminal.ansiBrightWhite": "#e8e6e6",
      "activityBar.background": "#0F140B",
      "activityBar.foreground": "#e8e6e6",
      "sideBarSectionHeader.background": "#0F140B",
