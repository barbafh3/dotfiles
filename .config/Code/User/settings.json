      "gitDecoration.modifiedResourceForeground": "#969592",
      "gitDecoration.deletedResourceForeground": "#807E78",
      "gitDecoration.untrackedResourceForeground": "#868579"
    }
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "variable.other",
        "settings": {
          "foreground": "#75a7a5"
        }
      },
      {
        "scope": "variable.other.object.cs",
        "settings": {
          "foreground": "#f4a701"
        }
      },
      {
        "scope": "entity.name",
        "settings": {
          "foreground": "#88afaf"
        }
      },
      {
        "scope": "entity.name.variable.parameter.cs",
        "settings": {
          "foreground": "#ffd6ad"
        }
      },
      {
        "scope": "entity.name.type",
        "settings": {
          "foreground": "#ffd6ad"
        }
      },
      {
        "scope": "entity.name.function",
        "settings": {
          "foreground": "#adad00"
        }
      },
      {
        "scope": "storage.type.cs",
        "settings": {
          "foreground": "#f4a701"
        }
      },
      {
        "scope": "keyword.type.cs",
        "settings": {
          "foreground": "#f4a701"
        }
      },
      {
        "scope": "keyword.other.new.cs",
        "settings": {
          "foreground": "#f80"
        }
      },
      {
        "scope": "keyword.other.var.cs",
        "settings": {
          "foreground": "#f4a701"
        }
      }
    ]
  }
}